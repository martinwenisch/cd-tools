{
    "env": {
        "AIRTABLE_API_KEY": "@airtable-api-key",
        "APITALKS_API_KEY": "@apitalks-api-key",
        "SLACK_TOKEN": "@slack-token"
    },
    "redirects": [
        {
            "source": "/",
            "destination": "https://github.com/cesko-digital/cd-tools"
        }
    ],
    "rewrites": [
        {
            "source": "/list-users",
            "destination": "/api/list-users"
        },
        {
            "source": "/api/nedluzimstatu/ovm/(.*)",
            "destination": "/api/nedluzimstatu/ovm"
        }
    ],
    "github": {
        "silent": true
    }
}